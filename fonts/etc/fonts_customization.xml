<?xml version="1.0" encoding="utf-8"?>
<fonts-modification version="1">

    <!-- GoogleSans -->
    <family customizationType="new-named-family" name="googlesans">
        <font weight="400" style="normal">GoogleSans-Regular-VF.ttf
            <axis tag="GRAD" stylevalue="0"/>
            <axis tag="opsz" stylevalue="18.0"/>
            <axis tag="wght" stylevalue="400"/>
        </font>
        <font weight="500" style="normal">GoogleSans-Regular-VF.ttf
            <axis tag="GRAD" stylevalue="0"/>
            <axis tag="opsz" stylevalue="18.0"/>
            <axis tag="wght" stylevalue="500"/>
        </font>
        <font weight="600" style="normal">GoogleSans-Regular-VF.ttf
            <axis tag="GRAD" stylevalue="0"/>
            <axis tag="opsz" stylevalue="18.0"/>
            <axis tag="wght" stylevalue="600"/>
        </font>
        <font weight="700" style="normal">GoogleSans-Regular-VF.ttf
